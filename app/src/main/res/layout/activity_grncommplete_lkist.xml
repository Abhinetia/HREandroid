<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
    android:background="#E6DDCD"
    app:cardUseCompatPadding="true"
    app:cardCornerRadius="10dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_margin="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/rv_cosepcn"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="140dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tvgrnnumber"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="120dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="PCN"
                android:minWidth="140dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_pcnno"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="120dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="PCN"
                android:minWidth="140dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_pcnnodet"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="120dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Indent no"
                android:minWidth="140dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_indentnogen"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="120dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Material Name"
                android:minWidth="140dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_matrildispaly"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="120dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Material Category"
                android:minWidth="140dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_matrilcategrory"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="120dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Brand"
                android:minWidth="140dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_brnddispaly"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="120dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Qty Raised"
                android:minWidth="140dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_dispatched"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="120dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Status"
                android:minWidth="140dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_ststu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="120dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Qty Recieved"
                android:minWidth="140dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_qtyrecved"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="120dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Qty Dispatched"
                android:minWidth="140dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_qtydispatched"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:minWidth="120dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Qty Pending"
                android:minWidth="140dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_qtypening"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:minWidth="120dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Dispatch Comment"
                android:minWidth="140dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_dispatchcomnet"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:minWidth="120dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Receiver Comment"
                android:minWidth="140dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_recvievercomnet"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:minWidth="120dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Information"
                android:minWidth="140dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="120dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>



        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:id="@+id/linearfrqty"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:weightSum="3"
                    android:layout_height="wrap_content">


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="5dp"
                        android:padding="5dp"
                        android:visibility="gone"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="5dp"
                            android:layout_alignParentStart="true"
                            android:textAllCaps="true"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:background="@drawable/ic_white"
                            android:id="@+id/tv_qtytotal"
                            android:textSize="11dp"
                            android:text="30"
                            android:textColor="@color/black_text"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="5dp"
                            android:layout_gravity="center"
                            android:id="@+id/tv_timee"
                            android:layout_marginBottom="10dp"
                            android:textSize="12dp"
                            android:text="Total"
                            android:textColor="#3E3E3E"
                            />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="5dp"
                        android:padding="5dp"
                        android:visibility="gone"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="5dp"
                            android:layout_alignParentStart="true"
                            android:textAllCaps="true"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:background="@drawable/ic_babypinkboreder"
                            android:id="@+id/tv_pening"
                            android:textSize="11dp"
                            android:text="30"
                            android:textColor="@color/black_text"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="5dp"
                            android:layout_gravity="center"
                            android:layout_marginBottom="10dp"
                            android:textSize="12dp"
                            android:text="Pending"
                            android:textColor="#3E3E3E"
                            />



                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:padding="5dp"
                    android:weightSum="2"
                    android:layout_marginBottom="10dp"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="10dp"
                        android:id="@+id/tvpending"
                        android:gravity="center"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:autoSizeTextType="uniform"
                        android:layout_gravity="center"
                        android:textSize="13dp"
                        android:background="@drawable/icrectborder"
                        android:text="Cancel"
                        android:textColor="#FF5E5E"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="10dp"
                        android:autoSizeTextType="uniform"
                        android:clickable="true"
                        android:gravity="center"
                        android:id="@+id/tv_viewdteuils"
                        android:layout_weight="1"
                        android:layout_marginRight="10dp"
                        android:layout_gravity="center"
                        android:textSize="13dp"
                        android:background="@drawable/icveiwdetails"
                        android:text="Receive Quantity"
                        android:textColor="@color/white"
                        />

                </LinearLayout>

            </LinearLayout>


        </androidx.cardview.widget.CardView>

    </LinearLayout>


</androidx.cardview.widget.CardView>