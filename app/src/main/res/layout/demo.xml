<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="wrap_content"
        android:id="@+id/rv_cosepcn"
        >

       <LinearLayout
           android:layout_width="match_parent"
           android:layout_margin="5dp"
           android:layout_height="wrap_content"
           android:orientation="horizontal">

           <TextView
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:text="GRN"
               android:minWidth="100dp"
               android:textColor="#FF9797"
               android:textSize="14sp" />

           <TextView
               android:id="@+id/tvgrnnumber"
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:text="GRN"
               android:minWidth="100dp"
               android:textColor="@color/black_text"
               android:textSize="14sp" />


       </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="100dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_pcnno"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="100dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="100dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_indentnogen"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="100dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="100dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_matrildispaly"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="100dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="100dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_brnddispaly"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GRN"
                android:minWidth="100dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />


        </LinearLayout>



    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_margin="10dp"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:id="@+id/linearfrqty"
            android:background="@color/hre_header"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:weightSum="3"
                android:background="@color/hre_header"
                android:layout_height="wrap_content">


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="5dp"
                    android:padding="5dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="5dp"
                        android:layout_alignParentStart="true"
                        android:textAllCaps="true"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:background="@drawable/ic_white"
                        android:id="@+id/tv_qtytotal"
                        android:textSize="11dp"
                        android:text="30"
                        android:textColor="@color/black_text"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="5dp"
                        android:layout_gravity="center"
                        android:id="@+id/tv_timee"
                        android:layout_marginBottom="10dp"
                        android:textSize="12dp"
                        android:text="Quantity"
                        android:textColor="#3E3E3E"
                        />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="5dp"
                    android:padding="5dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="5dp"
                        android:layout_gravity="center"
                        android:layout_alignParentStart="true"
                        android:textAllCaps="true"
                        android:gravity="center"
                        android:background="@drawable/ic_greenbaby"
                        android:id="@+id/tv_qtydeliveredl"
                        android:textSize="11dp"
                        android:text="0"
                        android:textColor="@color/black_text"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="5dp"
                        android:layout_marginBottom="10dp"
                        android:textSize="12dp"
                        android:layout_gravity="center"
                        android:text="Delivered"
                        android:textColor="#3E3E3E"
                        />


                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="5dp"
                    android:padding="5dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="5dp"
                        android:layout_alignParentStart="true"
                        android:textAllCaps="true"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:background="@drawable/ic_babypinkboreder"
                        android:id="@+id/tv_qydeliveredl"
                        android:textSize="11dp"
                        android:text="30"
                        android:textColor="@color/black_text"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="5dp"
                        android:layout_gravity="center"
                        android:layout_marginBottom="10dp"
                        android:textSize="12dp"
                        android:text="Pending"
                        android:textColor="#3E3E3E"
                        />


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="5dp"
                android:padding="5dp"
                android:weightSum="2"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:gravity="center"
                    android:layout_marginRight="10dp"
                    android:layout_weight="1"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:background="@drawable/icrectborder"
                    android:text="Status Pending"
                    android:textColor="#FF5E5E"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:background="@drawable/icveiwdetails"
                    android:text="View Details"
                    android:textColor="@color/white"
                    />

            </LinearLayout>

        </LinearLayout>


    </androidx.cardview.widget.CardView>

    </LinearLayout>


</androidx.cardview.widget.CardView>