<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
    app:cardUseCompatPadding="true"
    app:cardCornerRadius="10dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_margin="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/rv_cosepcn"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Date"
                android:gravity="center"
                android:minWidth="80dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tvgrnnumber"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="LOGIN"
                android:gravity="center"
                android:minWidth="80dp"
                android:textColor="#FF9797"
                android:textSize="14sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Logout"
                android:minWidth="80dp"
                android:gravity="center"
                android:textColor="#FF9797"
                android:textSize="14sp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Working Time"
                android:minWidth="80dp"
                android:gravity="center"
                android:textColor="#FF9797"
                android:textSize="14sp" />



        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_displaydaee"
                android:minWidth="80dp"
                android:textSize="14dp"
                android:text="09-05-2023"
                android:textColor="@color/black_text" />

            <TextView
                android:id="@+id/tv_tmee"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="10:24 am"
                android:minWidth="80dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_timee"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="10:24 am"
                android:minWidth="80dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />
            <TextView
                android:id="@+id/tv_lotimee"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="10:24 am"
                android:minWidth="80dp"
                android:textColor="@color/black_text"
                android:textSize="14sp" />

        </LinearLayout>


    </LinearLayout>


</androidx.cardview.widget.CardView>